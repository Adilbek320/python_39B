define(["jquery"],function($){"use strict";$.widget("coke.globalBanner",{_create:function(){var self=this,index=0,messages=self.element.find(".banner-message");$(messages[0]).addClass("active");return setInterval(function(){$(messages[index]).removeClass("active");if(index>=messages.length-1)index=0;else index++;$(messages[index]).addClass("active");},5000);},});return $.coke.globalBanner;});